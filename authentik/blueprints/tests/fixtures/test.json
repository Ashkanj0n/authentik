            }
        },
        {
            "model": "authentik_core.application",
            "identifiers": {
                "slug": "test-json"
            },
            "attrs": {
                "name": "test-json",
                "provider": {
                    "goauthentik.io/yaml-key": "!KeyOf",
                    "args": "provider"
                }
            }
        }
    ]
